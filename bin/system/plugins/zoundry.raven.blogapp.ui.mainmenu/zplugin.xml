                <group>zoundry.blogapp.ui.core.menugroup.file.new</group>
                <name>%menuitem.file.new.mediastorage.name%</name>
                <gravity>30</gravity>
                <description>%menuitem.file.new.mediastorage.description%</description>
                <icon>icons/mainmenu/file/new/mediastorage.png</icon>
            </menu-item>
        </extension-data>
    </zoundry-extension>
    <!-- File->New->Blog Site Menu Item -->
    <zoundry-extension point="zoundry.appframework.ui.core.menuitem">
