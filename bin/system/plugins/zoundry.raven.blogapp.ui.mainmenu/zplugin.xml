<?xml version="1.0" encoding="UTF-8"?>
<zoundry-plugin xmlns="http://www.zoundry.com/schemas/2006/03/zplugin.rng" version="1.0" signature="" id="zoundry.raven.blogapp.ui.mainmenu" name="Zoundry Main Menu">

    <dependencies>
        <depends on="zoundry.raven.blogapp.services" version="1.0"/>
        <depends on="zoundry.raven.blogapp.extensions" version="1.0"/>
    </dependencies>

    <!-- File->New->Blog Post Menu Item -->
    <zoundry-extension point="zoundry.appframework.ui.core.menuitem">
        <id>zoundry.blogapp.ui.core.menuitem.file.new.blog-post</id>
        <class>zoundry.blogapp.ui.menus.main.file_new.ZNewBlogPostMenuAction</class>
        <extension-data>
            <menu-item>
                <group>zoundry.blogapp.ui.core.menugroup.file.new</group>
                <name>%menuitem.file.new.blogpost.name%</name>
                <gravity>5</gravity>
                <description>%menuitem.file.new.blogpost.description%</description>
                <icon>icons/mainmenu/file/new/blogpost.png</icon>
            </menu-item>
        </extension-data>
    </zoundry-extension>
    <!-- File->New->[Separator] Menu Item -->
    <zoundry-extension point="zoundry.appframework.ui.core.menuitem">
        <id>zoundry.blogapp.ui.core.menuitem.file.new.separator1</id>
        <extension-data>
            <menu-item>
                <group>zoundry.blogapp.ui.core.menugroup.file.new</group>
                <name>SEPARATOR</name>
                <gravity>10</gravity>
                <type>separator</type>
            </menu-item>
        </extension-data>
    </zoundry-extension>
    <!-- File->New->Media Storage Menu Item -->
    <zoundry-extension point="zoundry.appframework.ui.core.menuitem">
        <id>zoundry.blogapp.ui.core.menuitem.file.new.media-storage</id>
