<?xml version="1.0" encoding="UTF-8"?>
<zoundry-plugin xmlns="http://www.zoundry.com/schemas/2006/03/zplugin.rng" version="1.0" signature="" id="zoundry.raven.blogapp.ui.browse" name="Zoundry Browse Perspective UI">

    <dependencies>
        <depends on="zoundry.raven.blogapp.services" version="1.0"/>
        <depends on="zoundry.raven.blogapp.ui.core" version="1.0"/>
    </dependencies>

    <!-- The Browse Perspective -->
    <zoundry-extension point="zoundry.blogapp.ui.perspective">
        <id>zoundry.blogapp.ui.perspectives.browse</id>
        <class>zoundry.blogapp.ui.perspectives.browse.ZBrowsePerspective</class>
        <extension-data>
            <perspective-info>
                <name>%perspective.browse.name%</name>
                <description>%perspective.browse.description%</description>
            </perspective-info>
        </extension-data>
    </zoundry-extension>

</zoundry-plugin>